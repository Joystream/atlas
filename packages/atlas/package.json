    "awesome-debounce-promise": "^2.1.0",
    "axios": "^0.27.2",
    "bezier-easing": "^2.1.0",
    "blake3": "2.1.7",
    "bn.js": "^5.2.1",
    "buffer": "^6.0.3",
    "comlink": "^4.3.1",
    "cropperjs": "^1.5.12",
    "date-fns": "^2.29.2",
    "downshift": "^6.1.9",
    "glider-js": "^1.7.7",
    "graphql": "^16.6.0",
    "graphql-tag": "^2.12.6",
    "haversine-distance": "^1.2.1",
    "history": "^5.3.0",
    "immer": "^9.0.15",
    "localforage": "^1.10.0",
    "lodash-es": "^4.17.21",
    "multihashes": "^4.0.3",
    "postcss-syntax": "^0.36.2",
    "rc-slider": "^10.0.1",
    "react": "^18.2.0",
