    "@types/react-datepicker": "^4.4.2",
    "@types/react-dom": "^18.0.6",
    "@types/react-helmet": "^6.1.5",
    "@types/react-transition-group": "^4.4.5",
    "@types/scroll-lock": "^2.1.0",
    "@types/video.js": "^7.3.46",
    "@vitejs/plugin-react": "^2.1.0",
    "esbuild-register": "^3.3.3",
    "happy-dom": "^4.1.0",
